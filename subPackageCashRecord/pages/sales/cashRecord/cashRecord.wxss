@import '../../../../assets/css/common.wxss';
.container{
  padding: 20rpx 32rpx;
  height: 100%;
  background-color: #f8f8f8;
}
page{
  height:100%;
}
.s-cashRecord-list-content{
  width: 686rpx;
  height: 228rpx;
  background: #fff;
  border-radius: 12rpx;
  padding: 20rpx 32rpx;
  box-sizing: border-box;
  color: #1D1E25;
  opacity: 0.88;
  font-size: 24rpx;
  letter-spacing: 4rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20rpx;
}
.s-cashRecord-time,
.s-cashRecord-status,
.s-cashRecord-list-title{
  display: flex;
  width: 100%;
  height: 92rpx;
  justify-content: space-between;
  align-items: center;
}
.s-cashRecord-time{
  margin-top: 20rpx;
}