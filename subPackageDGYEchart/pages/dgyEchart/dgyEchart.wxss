@import "../../../assets/css/common.wxss";
@import "../../../assets/css/chart.wxss";
.container{
  box-sizing: border-box;
  padding-bottom: 20rpx;
  height: 100%;
}
page{
  background-color: #2B2B31;
}
.dgy-chart2-content{
  height: 700rpx;
}
.pie-box{
  display:flex;
  height:600rpx;
  width: 686rpx;
  box-sizing: border-box;
}
.pie-content{
  width: 60%;
  height: 100%;
  /* padding-left:0px; */
}
.pie-msg{
  width: 40%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.pie-msg>view{
  white-space: nowrap;
  margin: 30rpx 0rpx;
  color: #fff;
  font-size: 28rpx;
  padding-right: 40rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
}
.pie-msg>span{
  white-space: nowrap;
  font-size: 30rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: #007DFF;
}