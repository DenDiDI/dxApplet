/* subPackageCamera/pages/scanCamera/scanCamera.wxss */
.s-code-input{
  width: 100%;
  height: 100rpx;
  position: relative; 
  background-color:#fff;
  padding:20px;
  box-sizing: border-box;
  display:flex;
  margin-bottom:20rpx;
  align-items: center;
  justify-content: space-around;
}
.s-code-scan{
  width: 220rpx;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  color:  #fff;
  display: flex;
  padding:4rpx;
  flex-direction: row;
  justify-content: center;
  border-radius: 10px;
  background-color: #e55530;
}